		{
			"name": "Create New User",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Generar valores aleatorios",
							"const randomUsername = `user_${Math.random().toString(36).substring(2, 10)}`;",
							"const randomEmail = `email_${Math.random().toString(36).substring(2, 10)}@example.com`;",
							"const randomPassword = Math.random().toString(36).substring(2, 12);",
							"",
							"// Almacenar username y password en el entorno",
							"pm.environment.set(\"randomUsername\", randomUsername);",
							"pm.environment.set(\"randomPassword\", randomPassword);",
							"",
							"// Registrar los valores generados",
							"console.log(\"Generated username:\", randomUsername);",
							"console.log(\"Generated email:\", randomEmail);",
							"console.log(\"Generated password:\", randomPassword);",
							"",
							"// Usar las variables en el body de la solicitud",
							"pm.variables.set(\"randomEmail\", randomEmail);",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response status code is 201\", function () {",
							"  pm.response.to.have.status(201);",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"username\": \"{{randomUsername}}\",\n    \"email\": \"{{randomEmail}}\",\n    \"password\": \"{{randomPassword}}\"\n}\n",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "http://localhost:8081/users/create",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8081",
					"path": [
						"users",
						"create"
					]
				}
			},
			"response": []
		},
		{
			"name": "Log In",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Validar que las variables de entorno existan",
							"if (!pm.environment.get(\"randomUsername\") || !pm.environment.get(\"randomPassword\")) {",
							"    console.error(\"randomUsername or randomPassword not found in environment!\");",
							"    throw new Error(\"Please make sure randomUsername and randomPassword are set in the environment.\");",
							"}",
							"",
							"// Establecer las variables en el body de la solicitud",
							"pm.variables.set(\"username\", pm.environment.get(\"randomUsername\"));",
							"pm.variables.set(\"password\", pm.environment.get(\"randomPassword\"));"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {",
							"    const response = pm.response.json();",
							"    if (response.access_token) {",
							"        // Guardar el access_token en el entorno",
							"        pm.environment.set(\"access_token\", response.access_token);",
							"        console.log(\"Access token saved:\", response.access_token);",
							"    } else {",
							"        console.error(\"Access token not found in response!\");",
							"        throw new Error(\"Access token is missing in the login response.\");",
							"    }",
							"} else {",
							"    console.error(\"Login failed with status code:\", pm.response.code);",
							"    throw new Error(\"Login request failed.\");",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"username\": \"{{username}}\",\n    \"password\": \"{{password}}\"\n}",
